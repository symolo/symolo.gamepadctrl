{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "types": [
    ],
    "lib": [
      "ES6","dom"
    ],
    "outDir": "lib"
  },
  "include": [
    "src/**/*.ts"
  ],
  "typedocOptions": {
      "entryPoints": ["src/"],
      "out": "docs",
      "entryPointStrategy": "expand",
      "readme": "./README.md"
  }
}
